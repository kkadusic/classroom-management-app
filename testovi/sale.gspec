

@objects
    meni      	  .meni
    logo          .meni img
	linkovi       #linkovi
	listItem      ul li
    sadrzaj       .sadrzaj
	tabelaSala    .tabelaSala
	headerSprat   .headerSprat
	headerRedni   .headerRedni
	slobodnaSala  .slobodnaSala
	zauzetaSala   .zauzetaSala
	sala   		  .sala
	kancelarija	  .kancelarija
	
= Meni =
	meni:
		contains linkovi
		height > 20px
		
    logo:
        above sadrzaj
		centered horizontally inside sadrzaj 50px
		height 20 to 100px
		width 20 to 100px
		
	linkovi: 
		below logo
		
	listItem:
		css display is "inline"		
		
= Sadrzaj =

	sadrzaj:
		below meni
		
	tabelaSala:
		centered horizontally inside sadrzaj
		contains slobodnaSala, zauzetaSala, kancelarija
		
	headerSprat:
		text is "Spratovi"
		height > 0px
		
	headerRedni: 
		text is "Redni broj"
		height > 0px
		
		